@model SDDFOS2021V1.Models.Admin

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <div>
        <h4>Admin</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                <b>Name</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminName)
            </dd>

            <dt>
                <b>Contact Number</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminContactNumber)
            </dd>

            <dt>
                <b>Email</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminEmail)
            </dd>

            <dt>
                <b>Username</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminUsername)
            </dd>

            <dt>
                <b>Password</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminPassword)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.AdminID }, new { @class = "btn btn-info btn-large" }) |
        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary btn-large" })
    </p>
</body>
</html>
