@model SDDFOS2021V1.Models.Admin

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h2>Are you sure you want to delete this?</h2>
    <div>

        <hr />
        <dl class="dl-horizontal">
            <dt>
                <b>Name</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminName)
            </dd>

            <dt>
                <b>Contact Number</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminContactNumber)
            </dd>

            <dt>
                <b>Email</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminEmail)
            </dd>

            <dt>
                <b>Username</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminUsername)
            </dd>

            <dt>
                <b>Password</b>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AdminPassword)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-danger" /> |
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary btn-large" })
            </div>
        }
    </div>
</body>
</html>
